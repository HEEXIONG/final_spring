<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.mapper.UsersMapper">


<insert id="userInsert">

insert into users (USER_NO,USER_ID,USER_PW,USER_NICKNAME,USER_PHONE,USER_EMAIL,USER_POST,USER_ADDR1,USER_ADDR2) 
VALUES(user_seq.nextval,#{USER_ID},#{USER_PW},#{USER_NICKNAME},#{USER_PHONE},#{USER_EMAIL},#{USER_POST},#{USER_ADDR1},#{USER_ADDR2})
</insert>



</mapper>